<h2>Login page</h2>
<form {{action 'authenticate' on='submit'}}>
  {{input value=login placeholder='Login'}}<br>
  {{input value=password placeholder='Password' type='password'}}<br>
  <button type="submit">Login</button>
  {{link-to "Register" 'register'}}

</form>
<button {{action 'googleLogin'}}>Login with GOOGLE</button>
<button {{action 'facebookLogin'}}>Login with Facebook</button>
<button {{action 'githubLogin'}}>Login with Github</button>